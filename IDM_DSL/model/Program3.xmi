<?xml version="1.0" encoding="UTF-8"?>
<iDM_DSL:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:iDM_DSL="http://www.example.org/iDM_DSL">
  <instruction
      xsi:type="iDM_DSL:FunctionDeclaration">
    <function
        name="entry">
      <instruction
          xsi:type="iDM_DSL:FunctionCall">
        <function
            name="setspeed">
          <parameter
              value="150"
              unit="MM"/>
        </function>
      </instruction>
      <instruction
          xsi:type="iDM_DSL:VariableDeclaration">
        <variable
            name="const"
            type="number"/>
      </instruction>
      <instruction
          xsi:type="iDM_DSL:VariableAssignement">
        <variable
            name="const"
            type="number">
          <expression
              xsi:type="iDM_DSL:ArithmeticExpr"/>
        </variable>
      </instruction>
    </function>
  </instruction>
</iDM_DSL:Program>
