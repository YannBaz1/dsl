 <?xml version="1.0" encoding="UTF-8"?>
<iDM_DSL:Program
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:iDM_DSL="http://www.example.org/iDM_DSL"
    xsi:schemaLocation="http://www.example.org/iDM_DSL DSL_IDM.ecore">
  <instruction
      xsi:type="iDM_DSL:FunctionDeclaration">
    <function
        name="entry">
      <instruction
          xsi:type="iDM_DSL:FunctionCall">
        <function
            name="setspeed">
          <parameter
              value="150"
              unit="mm"/>
        </function>
      </instruction>
     
      <instruction
          xsi:type="iDM_DSL:Loop">
        <instruction
            xsi:type="iDM_DSL:FunctionCall">
          <function
              name="square"/>
        </instruction>
        <booleanexpr
            operator="LESS">
          <newvariable
              name="count"
              type="number"/>
          <expression
              xsi:type="iDM_DSL:IntExpression"
              value="5"/>
        </booleanexpr>
      </instruction>
     
    </function>
  </instruction>
  <instruction
      xsi:type="iDM_DSL:FunctionDeclaration">
    <function
        name="square">
      <instruction
          xsi:type="iDM_DSL:Movement"
          Distance="30"
          unit="cm"/>
      <instruction
          xsi:type="iDM_DSL:Rotation"
          angleValue="90.0"/>
    </function>
  </instruction>
</iDM_DSL:Program> 
